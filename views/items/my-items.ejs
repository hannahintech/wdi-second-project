<% if (locals.isLoggedIn) { %>
  <div class="container">
    <h3>all <%= `${locals.user.username}'s` %> added items </h3>
  </div>
  <ul class="container">
    <div class="row">
      <% items.forEach((item) => { %>

        <% if(item.createdBy && item.createdBy.username === locals.user.username) { %>
        <li class"col-lg-2">
        <a href="/items/<%= item.id %>">
          <div class="item-image-div">
            <img class="outfit-image" src="<%= item.image %>">
          </div>
        </a>
        </li>
        <% } %>

      <% }) %>
  </div>
  </ul>
<% }; %>
