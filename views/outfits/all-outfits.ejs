<div class="container">
  <h1>Saved Outfits</h1>
</div>
  <ul class="container">
    <div class="row">
      <% items.forEach((item) => { %>
        <li class"col-lg-2">
        <img class="outfit-image" src="<%= item.image %>">
          <%# <p>Generated by:</p>(id of user who generated this => attained from "generate outfit button" view G, && saved "button clicked" view H) %>
        </li>
      <% }) %>
    </div>
  </ul>

  <ul class="container">
    <div class="row">
      <% category.forEach((category) => { %>
        <li class"col-lg-2">
        <p><%= category %></p>
        </li>
      <% }) %>
    </div>
  </ul>
